{
  "expo": {
    "name": "Habbit Tracker",
    "slug": "habbittracker-app",
    "version": "0.1.0",
    "icon": "./src/components/Icon.png",
    "android": {
      "package": "com.lysara.habbittracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/components/Icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "ios": {
      "bundleIdentifier": "com.lysara.habbittracker",
      "buildNumber": "1",
      "icon": "./src/components/Icon.png"
    },
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "scheme": "habbittracker",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "eas": {
        "projectId": "d5916848-b63e-4833-933a-62a33f2fa5d7"
      }
    },
    "plugins": [
      "expo-font",
      "expo-sqlite"
    ]
  }
}
